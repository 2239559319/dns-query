    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch current file",
        "program": "${file}",
        "request": "launch",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "type": "pwa-node"
      },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch app",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/index.js",
        }
    ]
}